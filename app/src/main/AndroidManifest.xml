<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tianxing.magic">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".base.MyApplication"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.order.SubbranchActivity"></activity>
        <activity android:name=".activity.communication.InterflowActivity"></activity>
        <activity android:name=".activity.recommend.HairStyleRecommendActivity"></activity>
        <activity android:name=".activity.shop.StoreInfoActivity"></activity>
        <activity android:name=".activity.user.PersonalCenterActivity"></activity>
        <activity android:name=".activity.activities.StoreActivityActivity"></activity>
        <activity android:name=".activity.order.DesignerActivity"></activity>
        <activity android:name=".activity.order.ProjectActivity"></activity>
        <activity android:name=".activity.order.TimeActivity"></activity>

    </application>

</manifest>